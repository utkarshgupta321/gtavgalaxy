<!DOCTYPE html>
<html>
<head><title>Admin Panel</title></head>
<body>
  <h1>Admin Dashboard</h1>
  <h2>Users</h2>
  <ul>
    <% users.forEach(user => { %>
      <li><%= user.username %> (<%= user.email %>) - <a href="/admin/delete-user/<%= user.id %>">Delete</a></li>
    <% }) %>
  </ul>
  <h2>Threads</h2>
  <ul>
    <% threads.forEach(thread => { %>
      <li><%= thread.title %> - <a href="/admin/delete-thread/<%= thread.id %>">Delete</a></li>
    <% }) %>
  </ul>
</body>
</html>
